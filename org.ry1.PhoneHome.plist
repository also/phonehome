<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<false/>
	<key>Label</key>
	<string>org.ry1.PhoneHome</string>
	<key>ProgramArguments</key>
	<array>
		<string>ruby</string>
		<string>/etc/phonehome.rb</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardOutPath</key>
	<string>/var/log/phonehome.log</string>
	<key>StartInterval</key>
	<integer>60</integer>
</dict>
</plist>
